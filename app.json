{
  "expo": {
    "name": "Do.it",
    "slug": "Doit",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/doiticon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/doitsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/doiticon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "nav.expo.doit"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
